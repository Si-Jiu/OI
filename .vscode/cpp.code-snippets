      "{",
      "    bool changed = true;",
      "    while (changed)",
      "    {",
      "        changed = false;",
      "        for (int i = 0; i < len; i++)",
      "        {",
      "            if (arr[i] > arr[i + 1])",
      "            {",
      "                swap(arr[i], arr[i + 1]);",
      "                changed = true;",
      "            }",
      "        }",
      "    }",
      "}"
